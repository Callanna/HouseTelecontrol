<resources>

    <!-- Base application theme. -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorAccent">@color/accent</item>
    </style>

    <style name="seekBarStyleHorizontal">
        <item name="android:focusable">false</item>
        <item name="android:maxHeight">6.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/seekbar_progress_horizontal</item>
        <item name="android:minHeight">6.0dip</item>
        <item name="android:thumb">@drawable/progress_dot_normal</item>
    </style>
</resources>
